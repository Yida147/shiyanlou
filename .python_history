def connect(ipaddress, port):
    print("IP: ", ipaddress)
    print("Port: ", port)
connect('192.168.1.1', 22)
connect(22, '192.168.1.1')
connect(prot=22, ipaddress='192.168.1.1')
connect(port=22, ipaddress='192.168.1.1')
